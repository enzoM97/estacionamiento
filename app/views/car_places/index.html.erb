<p id="notice"><%= notice %></p>

<h1>Listing Car Places</h1>

<table>
  <thead>
    <tr>
      <th>Hora entrada</th>
      <th>Hora salida</th>
      <th>Fecha entrada</th>
      <th>Fecha salida</th>
      <th>Car</th>
      <th>Place</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_places.each do |car_place| %>
      <tr>
        <td><%= car_place.hora_entrada %></td>
        <td><%= car_place.hora_salida %></td>
        <td><%= car_place.fecha_entrada %></td>
        <td><%= car_place.fecha_salida %></td>
        <td><%= car_place.car %></td>
        <td><%= car_place.place %></td>
        <td><%= link_to 'Show', car_place %></td>
        <td><%= link_to 'Edit', edit_car_place_path(car_place) %></td>
        <td><%= link_to 'Destroy', car_place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

